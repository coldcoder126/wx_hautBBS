.container{
  width: 100%;
  position: fixed;
  top: 0;
  box-shadow:1px -1px 1px #e3e3e3;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  z-index: 99;
  background-color: #fff;

}


.navi-container{
  width: 94%;
  height: 70rpx;
  background-color: #f2f2f2;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  border-radius: 6rpx;
}

.module{
width: 70%;
display: flex;
flex-direction: row;
justify-content: flex-start;
align-items: center;
  
}

.icon{
  width: 60rpx;
  display: flex;
  justify-content: center;
}

.in-module{
width: 30%;
margin: 10rpx 30rpx;
}

.search{
  display: block;
  height: 40rpx;
  width: 40rpx;
}
